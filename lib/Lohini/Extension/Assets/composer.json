{
	"name": "lohini/assets-extension",
	"description": "Integration of Assetic into Nette Framework",
	"keywords": ["nette", "assetic", "assets"],
	"homepage": "http://lohini.net",
	"authors": [
		{
			"name": "Filip Procházka",
			"homepage": "http://filip-prochazka.com"
		},
		{
			"name": "Pavol Hluchý",
			"homepage": "http://lopo.losys.eu"
		}
	],
	"require": {
		"nette/nette" : "*",
		"kriswallsmith/assetic": "dev-master"
	},
	"conflict": {
		"lohini/framework": "self.version"
	},
	"autoload": {
		"psr-0": {"Lohini\\Extension\\Assets": ""},
		"classmaps": [
			"exceptions.php"
		]
	},
	"target-dir":"Lohini/Extension/Assets"
}
